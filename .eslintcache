log(err));\n        }\n      \n\n    filterEmployees = (searchkey) => {\n      console.log(searchkey);\n      console.log(this.state.result);\n      var filterResult = this.state.result.filter(person => person.firstName.toLowerCase() === searchkey.toLowerCase())\n  \n      this.setState({\n        result:filterResult\n        \n      })\n    }\n  \n\n\n  render() {\n    // if (this.state.loading) {\n    //   return <div>loading...</div>;\n    // }\n\n    // if (!this.state.result) {\n    //   return <div>didn't get a person</div>;\n    // }\n\n    return (\n        \n        <div> \n           <SearchForm \n            type = {this.state.result}\n            // search={this.state.search}\n            handleFormSubmit={this.handleFormSubmit}\n            handleInputChange={this.handleInputChange}\n            value={this.state.search}\n          />\n            \n            {/* <People type = {this.state.result}/> */}\n            <div className=\"row\">\n          {/* <div > */}\n          <table className=\"table\">\n            <tr>\n              <th scope=\"col\">Photo</th>\n              <th>First Name</th>\n              {/* <th onClick={this.onSortChange}>First Name   */}\n              {/* <button onClick={this.onSortChange}> ^\n\t\t\t\t\t\t\t\t</button> */}\n              {/* </th> */}\n              <th scope=\"col\">Last Name </th>\n              <th scope=\"col\">Email</th>\n              <th scope=\"col\">Phone</th>\n            </tr>\n\n            {/* { [...this.state.result].sort(this.sortTypes[this.state.currentSort].fn).map((item) =>  */}\n            {/* {this.state.result.length > 0 ? (\n                <div>\n                  {this.state.result.map(item => (\n                    <EmployeeCard\n                      picture={item.picture}\n                      firstName={item.firstName}\n                      lastName={item.lastName}\n                      email={item.email}\n                      phone={item.phone}\n                      key={item.key}\n                    />\n                  ))}\n                </div>\n              ) : (<div />)} */}\n            {[...this.state.result].map((item) =>\n              <People\n                picture={item.picture}\n                firstName={item.firstName}\n                lastName={item.lastName}\n                email={item.email}\n                phone={item.phone}\n                key={item.key}\n              />\n            )}\n\n          </table>\n        </div>\n\n        \n        </div>\n    );\n  }\n}","/Users/luiscalderin007/Desktop/uconn coding bootcamp/conn-hfd-fsf-pt-07-2020-u-c/19-React/02-Homework/00-Unit_19/Employee-Directory/src/components/People.js",[],"/Users/luiscalderin007/Desktop/uconn coding bootcamp/conn-hfd-fsf-pt-07-2020-u-c/19-React/02-Homework/00-Unit_19/Employee-Directory/src/components/SearchForm.js",["48"],"import React from \"react\";\nimport People from \"./People\"\n\nfunction SearchForm(props) {\n  // if(props.type.name.first === props.search){\n  //   return(\n  //     <People/>\n  //   )\n  // }\n\n  return (\n    <form>\n      <div className=\"form-group\">\n        <label htmlFor=\"search\">Search:</label>\n        <input\n          onChange={props.handleInputChange}\n          value={props.value}\n          name=\"search\"\n          type=\"text\"\n          className=\"form-control\"\n          placeholder=\"Search first name only\"\n          id=\"search\"\n        />\n        <button onClick={props.handleFormSubmit} className=\"btn btn-primary mt-3\">\n          Search\n        </button>\n      </div>\n    </form>\n  );\n}\n\nexport default SearchForm;","/Users/luiscalderin007/Desktop/uconn coding bootcamp/conn-hfd-fsf-pt-07-2020-u-c/19-React/02-Homework/00-Unit_19/Employee-Directory/src/utils/API.js",["49"],"import axios from \"axios\";\n\n// const BASEURL = \"https://randomuser.me/api/?results=50\";\n// // const APIKEY = \"&api_key=dc6zaTOxFJmzC&limit=20\";\n\n// // Export an object with a \"search\" m